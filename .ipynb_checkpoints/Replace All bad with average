reviews = ["good", "bad", "excellent", "bad", "average"]
reviews_newlist = []
for i in range(len(reviews)):
    if reviews[i] == "bad":
        reviews_newlist.append("average")
    else:
        reviews_newlist.append(reviews[i])

print(reviews)
print(reviews_newlist)

