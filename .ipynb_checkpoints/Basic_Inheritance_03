class Person():
    def __init__(self, name, age):
        self.name = name
        self.age = age 
class Student(Person):
    def __init__(self, name, age, grade):
       super().__init__(name, age)
       self.grade = grade
    def display(self):
        print(self.name)
        print(self.age)
        print(self.grade)
s1 = Student("Huntrix", "12", "7th")
s1.display()

