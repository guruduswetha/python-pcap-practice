class Parentclass:
    def __init__(self, fname, lname):
        self.firstname = fname
        self.lastname = lname
    def printingmanually (thisway):
        print(thisway.firstname, thisway.lastname)

class Childclass(Parentclass):
    pass
p1 = Childclass("KAVEESH","GURUDU")
p1.printingmanually()
